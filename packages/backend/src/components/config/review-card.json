{
  "collectionName": "components_config_review_cards",
  "info": {
    "displayName": "Review Card",
    "icon": "star"
  },
  "options": {},
  "attributes": {
    "reviewer": {
      "type": "string",
      "required": true,
      "default": "John Doe"
    },
    "review": {
      "type": "text",
      "required": true,
      "default": "This is a review text."
    },
    "rating": {
      "type": "integer",
      "required": true,
      "default": 5,
      "min": 1,
      "max": 5
    },
    "date": {
      "type": "date",
      "required": false
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false
    }
  }
}
